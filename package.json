{
  "name": "react-mobile-boilerplate",
  "version": "0.7.0",
  "description": "A react boilerplate for mobile webapp",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node src/server/main.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/server/main.js",
    "start:hot": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack/webpack.config.dev.js",
    "dev": "concurrently --kill-others \"npm run start:hot\" \"npm run start:dev\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config webpack/webpack.config.server.js",
    "profile": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --profile --json > profile.json",
    "clean": "rimraf build && rimraf public",
    "eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-mobile-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "mobile"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-mobile-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-mobile-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss": "^7.0.16",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "react-hot-loader": "^4.8.6",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "svgo": "^1.2.2",
    "svgo-loader": "^2.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "concurrently": "^4.1.0",
    "connected-react-router": "^6.4.0",
    "express": "^4.17.0",
    "express-handlebars": "^3.1.0",
    "hahoorequest": "^0.6.7",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.0.3",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-act-reducer": "^2.1.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "webpack-isomorphic-tools": "^3.0.6"
  }
}
